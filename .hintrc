{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "box-align"
        ]
      }
    ],
    "no-inline-styles": "off",
    "meta-charset-utf-8": "off"
  }
}